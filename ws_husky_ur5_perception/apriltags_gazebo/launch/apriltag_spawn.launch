<?xml version="1.0" ?>

<launch>
<!--param name="apriltag" command="$(find xacro)/xacro inorder '$(find apriltags_gazebo)/urdf/apriltag.urdf.xacro'"/-->
<env name="GAZEBO_MODEL_PATH" value="$(find apriltags_gazebo)/models/"/>
  <!-- push robot_description to factory and spawn robot in gazebo -->
<node name="apriltag_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-gazebo -file '$(find apriltags_gazebo)/models/apriltag/model.sdf' -model apriltag -x 0.86 -y -0.095 -z 1.5 " />
<!--node name="table_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-database cafe_table -gazebo -model cafe_table -y -1 -z -0" /-->
<node name="satnd_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-gazebo -file '$(find apriltags_gazebo)/models/stand.sdf' -model stand -x 0.9 -y -0.095 -z -0.1" />
</launch>
