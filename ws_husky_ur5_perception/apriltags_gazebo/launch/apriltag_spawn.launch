<?xml version="1.0" ?>

<launch>
<!--param name="apriltag" command="$(find xacro)/xacro inorder '$(find apriltags_gazebo)/urdf/apriltag.urdf.xacro'"/-->
<env name="GAZEBO_MODEL_PATH" value="$(find apriltags_gazebo)/urdf/models/"/>
  <!-- push robot_description to factory and spawn robot in gazebo -->
<node name="apriltag_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-gazebo -file '$(find apriltags_gazebo)/models/apriltag/model.sdf' -model apriltag -x 0.26 -y 0.7 -z -0.05" />
<node name="table_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-database cafe_table -gazebo -model cafe_table -y -1 -z -0" />
<node name="ball_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-database cricket_ball -gazebo -model cricket_ball -x 0.32 -y -0.6 -z 3.5" />
</launch>